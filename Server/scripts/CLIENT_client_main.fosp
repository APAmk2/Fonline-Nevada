	};
	
	uint index=(type&0xFFFF);
	uint currentLevel=(type>>16);
	
	if(index>8)
	return false;
	data.resize(elevators[index].length());
	for(uint i=0;i<data.length();i++)
	data[i]=elevators[index][i];
	data[0]=currentLevel;
